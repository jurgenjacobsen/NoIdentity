<%- include('blocks/header', {client: client, auth: auth, user: user}) %>
    <%
        const help = {};
        client.commands.forEach((command) => {
            const cat = command.help.category;
            if(!help.hasOwnProperty(cat)) help[cat] = [];
            help[cat].push(command);
        });
    %>

    <div class="head">
        <h1 class="page-title"><%= client.commands.size%> Comandos</h1>
    </div>

    <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1440 320"><path fill="#26292b" fill-opacity="1" d="M0,160L48,176C96,192,192,224,288,202.7C384,181,480,107,576,106.7C672,107,768,181,864,192C960,203,1056,149,1152,122.7C1248,96,1344,96,1392,96L1440,96L1440,320L1392,320C1344,320,1248,320,1152,320C1056,320,960,320,864,320C768,320,672,320,576,320C480,320,384,320,288,320C192,320,96,320,48,320L0,320Z"></path></svg>

    <div style="background-color: #26292b; margin-top: -10%;">
        <div class="container" style="text-align: center;">
            <% for (const category in help) { %>
                <% if(category !== `Somente para desenvolvedores`) {%>
                    <h1 class="command-category-title"><%= category %></h1>
    
                    <div class="command-list">
                        <% for (const command of help[category]) { %>
                            <div class="command">
                                <div class="command-name">
                                    <%= command.help.name %>
                                </div>
                                <p>
                                    <hr>
                                    <a class="command-button" href="/commands/<%= command.help.name %>">Ver mais</a>
                                </p>
                            </div>
                        <% }; %>
                    </div>
                <% } %>
            <% }; %>
        </div>
    </div>
    <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1440 320"><path fill="#26292b" fill-opacity="1" d="M0,64L48,85.3C96,107,192,149,288,149.3C384,149,480,107,576,96C672,85,768,107,864,138.7C960,171,1056,213,1152,202.7C1248,192,1344,128,1392,96L1440,64L1440,0L1392,0C1344,0,1248,0,1152,0C1056,0,960,0,864,0C768,0,672,0,576,0C480,0,384,0,288,0C192,0,96,0,48,0L0,0Z"></path></svg>

<%- include('blocks/footer') %>